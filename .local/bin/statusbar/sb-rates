#!/bin/sh

# Opens crypto rates (rates.xs)

case $BLOCK_BUTTON in
	1) setsid -f "$TERMINAL" -e sh "$HOME/.local/bin/rates.sh" ;;
	2) setsid -f "$TERMINAL" -e sh "$HOME/.local/bin/rates.sh" ;;
	3) notify-send "Rates module" "\- Click/middle click to execute rates.sh." ;;
	6) "$TERMINAL" -e "$EDITOR" "$0" ;;
esac

printf "Ôçπ"
